    <ul id="todonav">
      <li><%= link_to "#{t('.show_all')} (#{@size_all})",  {todo_list_cond: :all}, data: {ignore: true} %></li>
      <li><%= link_to "#{t('.only_incomplete')} (#{@size_incomplete})", {todo_list_cond: :incomplete}, data: {ignore: true} %></li>
      <li><%= link_to "#{t('.only_complete')} (#{@size_complete})", {action: :index, todo_list_cond: :complete}, data: {ignore: true} %></li>
    </ul>
    <ul id="todolist">
      <% @todos.each do |todo| %>
          <li><%= link_to todo.title, todo, data: {ignore: true} %></li>
      <% end %>
    </ul>

